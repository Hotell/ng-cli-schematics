{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "application": {
      "factory": "./application",
      "schema": "./application/schema.json",
      "description": "Create an Angular application with better defaults."
    },
    "jest": {
      "factory": "./jest",
      "schema": "./jest/schema.json",
      "description": "Add configured Jest testing framework"
    },
    "component": {
      "aliases": ["c"],
      "factory": "./component",
      "description": "Create an Angular component.",
      "schema": "./component/schema.json"
    },
    "directive": {
      "aliases": ["d"],
      "factory": "./directive",
      "description": "Create an Angular directive.",
      "schema": "./directive/schema.json"
    },
    "module": {
      "aliases": ["m"],
      "factory": "./module",
      "description": "Create an Angular module.",
      "schema": "./module/schema.json"
    },
    "service": {
      "aliases": ["s"],
      "factory": "./service",
      "description": "Create an Angular service.",
      "schema": "./service/schema.json"
    }
  }
}
